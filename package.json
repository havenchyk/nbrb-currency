{
  "name": "nbrb-currency",
  "version": "1.0.3",
  "description": "Get currency list from website of national belarussian bank in json",
  "main": "index.es5.js",
  "jsnext:main": "index.js",
  "scripts": {
    "coverage": "istanbul cover _mocha -- --require babel/register",
    "precoveralls": "npm run coverage",
    "coveralls": "coveralls < coverage/lcov.info",
    "test": "mocha --compilers js:babel-core/register -t 0",
    "tdd": "npm test -- --watch",
    "prepush": "npm test",
    "transpile": "babel index.js > index.es5.js",
    "prepublish": "npm run transpile",
    "postpublish": "npm run rm && git push origin master --follow-tags",
    "rm": "rm index.es5.js",
    "lint": "standard index.js test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/havenchyk/nbrb-currency.git"
  },
  "author": "Uladzimir Havenchyk <havenchyk@gmail.com> (https://github.com/havenchyk)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/havenchyk/nbrb-currency/issues"
  },
  "homepage": "https://github.com/havenchyk/nbrb-currency#readme",
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-core": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-register": "^6.9.0",
    "chai": "^3.0.0",
    "coveralls": "^2.11.2",
    "husky": "^0.10.1",
    "istanbul": "^0.4.0",
    "mocha": "^2.2.4",
    "standard": "^7.1.2"
  },
  "dependencies": {
    "babel-polyfill": "^6.9.1",
    "bluebird": "^3.4.1",
    "isomorphic-fetch": "^2.2.0",
    "xml2js": "^0.4.8"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  }
}
