  },
  "homepage": "https://github.com/havenchyk/nbrb-currency#readme",
  "devDependencies": {
    "babel": "^5.2.16",
    "babel-eslint": "^4.0.10",
    "chai": "^3.0.0",
    "coveralls": "^2.11.2",
    "eslint": "^1.1.0",
    "husky": "^0.10.1",
    "istanbul": "^0.4.0",
    "mocha": "^2.2.4"
  },
